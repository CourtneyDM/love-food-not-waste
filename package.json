{
    "name": "waste-not",
    "version": "1.0.1",
    "description": "Love Food Not Waste project",
    "main": "server.js",
    "engines": {
        "node": "8.9.x",
        "npm": "5.4.x"
    },
    "scripts": {
        "server": "node server.js",
        "client": "cd server && node scripts/start-client.js",
        "start": "node server",
        "app": "concurrently \"cd server && nodemon --ignore sessions/ server.js \" \"cd server && npm run client\"",
        "build": "cd server && node scripts/build.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "heroku-postbuild": "cd client && npm install && npm run build"
    },
    "author": "'Lauren Abney', 'Avery Bruner', 'Jada Gonzales', 'Courtney Montgomery'",
    "license": "ISC",
    "dependencies": {
        "auth0-js": "^9.7.3",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "concurrently": "^3.6.0",
        "connect": "^3.6.6",
        "dotenv": "^6.0.0",
        "express": "^4.16.3",
        "express-session": "^1.15.6",
        "mime": "^2.3.1",
        "mongoose": "^5.2.4",
        "nodemon": "^1.18.3",
        
        "qs": "^6.5.2",
        "react-google-maps": "^9.4.5",
        "recompose": "^0.28.2",
        "session-file-store": "^1.2.0",
        "uuid": "^3.3.2"
    }
}
